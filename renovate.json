{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchManagers": ["pip_requirements", "pip_setup", "pipenv", "poetry", "pyproject"],
      "enabled": true
    }
  ],
  "prConcurrentLimit": 3,
  "prHourlyLimit": 2,
  "automerge": false
}
